{
  "name": "@albseb511/lotus-ui",
  "version": "0.1.3",
  "private": false,
  "access": "public",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public --quiet -o ./storybook-static",
    "chromatic": "chromatic --exit-zero-on-changes",
    "build": "rollup -c",
    "prePublishOnly": "rollup -c"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@material-ui/styles": "^4.11.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^24.0.24",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^10.0.0",
    "chromatic": "^5.9.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react": "^16.13.1",
    "react-aria": "^3.7.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.53.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albseb511/lotus-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/albseb511/lotus-ui/issues"
  },
  "homepage": "https://github.com/albseb511/lotus-ui#readme"
}
